<div class="col m3 l3">&nbsp;</div>
<div class="col l6 m6 s12 center">
  <h3>Sign Up</h3>
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <div class="input-field">
      <i class="material-icons prefix">email</i>
       <%= f.text_field :email, :placeholder=> "Email Address", :class=>"validate" %>
    </div>
    <div class="input-field">
       <i class="mdi-action-lock-outline prefix"></i>
       <%= f.password_field :password, :placeholder=> "Password", :class=>"validate" %>
    </div>
    <div class="input-field">
       <i class="mdi-action-lock prefix"></i>
       <%= f.password_field :password_confirmation, :placeholder=> "Confirm Password", :class=>"validate" %>
    </div>
    <div class="actions">
      <%= f.submit "Sign Up",:class=>"btn btn-success waves-effect waves-light"%>
    </div>
  <%end%>
  <div style="margin-top:10px;">
    Already Registered? <%= link_to "Log in", new_session_path(resource_name), :class=>"" %><br />
  </div>
  <%#= render "devise/shared/links" %>
</div>
<div class="col m3 l3">&nbsp;</div>

